	<queryString>
		<![CDATA[SELECT 
    a.cliente_id,
    b.nome,
    COUNT(a.cliente_id) AS 'qtde_pedidos',
    (SELECT 
            SUM(a1.valor_unitario)
        FROM
            pedido_tipo_roupa a1,
            pedido b1
        WHERE
            a1.pedido_id = b1.id
                AND b1.cliente_id = b.id
        GROUP BY b1.cliente_id) AS 'Receita'
FROM
    pedido a,
    cliente b
WHERE
    a.cliente_id = b.id
GROUP BY a.cliente_id
ORDER BY COUNT(a.cliente_id) DESC
LIMIT 5]]>
	</queryString>
